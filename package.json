{
	"name": "moonfin",
	"version": "2.0.1",
	"description": "Jellyfin client for LG webOS TV",
	"author": "RadicalMuffinMan",
	"main": "src/index.js",
	"scripts": {
		"serve": "enact serve",
		"pack": "enact pack",
		"build": "npm build.js",
		"watch": "enact pack --watch",
		"clean": "enact clean",
		"lint": "enact lint .",
		"license": "enact license",
		"test": "enact test",
		"test-watch": "enact test --watch",
		"install-tv": "ares-install ./build/*.ipk -d tv",
		"launch-tv": "ares-launch org.moonfin.webos -d tv"
	},
	"license": "MPL-2.0",
	"private": true,
	"repository": "https://github.com/Moonfin-Client/webOS",
	"overrides": {
		"fsevents": "$fsevents"
	},
	"enact": {
		"theme": "sandstone",
		"ri": {
			"baseSize": 24
		},
		"locales": [
			"en-US"
		],
		"ilib": false,
		"screenTypes": [
			{
				"name": "fhd",
				"pxPerRem": 24,
				"width": 1920,
				"height": 1080
			},
			{
				"name": "uhd",
				"pxPerRem": 48,
				"width": 3840,
				"height": 2160
			}
		]
	},
	"eslintIgnore": [
		"node_modules/*",
		"build/*",
		"dist/*"
	],
	"dependencies": {
		"@enact/core": "^4.8.0",
		"@enact/i18n": "^4.8.0",
		"@enact/sandstone": "^2.8.0",
		"@enact/spotlight": "^4.8.0",
		"@enact/ui": "^4.8.0",
		"@enact/webos": "^5.4.1",
		"hls.js": "^1.4.14",
		"ilib": "^14.19.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"web-vitals": "^3.5.1"
	},
	"devDependencies": {
		"@enact/cli": "^6.1.3",
		"@webosose/ares-cli": "^2.4.0",
		"baseline-browser-mapping": "^2.9.19",
		"eslint-config-enact-proxy": "^1.0.7"
	},
	"optionalDependencies": {
		"fsevents": "^2.3.2"
	}
}